<!DOCTYPE html>
<html>
<head>
  <title>GistClone</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% unless session[:user_id].nil? %>
    <%=link_to "Log Out", session_url(session[:user_id]), :method => :delete %>
  <% end %>
<%= yield %>

</body>
</html>
